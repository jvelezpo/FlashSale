    "@types/mocha": "^9.0.0",
    "@types/react": "17.0.19",
    "chai": "^4.3.4",
    "sinon": "^11.1.2",
    "chai-http": "^4.3.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.0",
    "mocha": "^9.1.2",
    "nodemon": "^2.0.13",
    "start-server-and-test": "^1.14.0",
