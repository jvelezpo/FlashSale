{
  "name": "profiles",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "worker": "dotenv -c -- node .next/server/worker.js",
    "dev:worker": "wait-on .next/server/worker.js && dotenv -c -- nodemon .next/server/worker.js -w src/server -w src/shared",
    "fixtures:up": "NODE_ENV=dev node .next/server/fixtures.js -u -c 100",
    "fixtures:down": "NODE_ENV=dev node .next/server/fixtures.js",
    "mocha": "mocha --exit './test/**/*.{mjs,ts}'",
    "e2e": "start-server-and-test dev http://localhost:3000 worker"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.0.15",
