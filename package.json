    "@next-auth/prisma-adapter": "^0.5.4",
    "@prisma/client": "^2.30.0",
    "amqplib": "^0.8.0",
    "dotenv-cli": "^4.0.0",
    "express-validator": "^6.12.2",
    "faker": "^5.5.3",
    "ioredis": "^4.27.10",
    "next": "^11.1.2",
    "next-auth": "^3.29.0",
    "prisma": "^2.30.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "^1.38.2"
    
  },
  "devDependencies": {
